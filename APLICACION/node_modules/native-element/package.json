{
  "name": "native-element",
  "version": "0.0.2",
  "description": "base web component",
  "main": "native-element.bundled.js",
  "module": "native-element.bundled.js",
  "type": "module",
  "scripts": {
    "lint": "lit-analyzer native-element.js && eslint '**/*.js'",
    "docs": "yarn docs:clean && yarn analyze && yarn docs:build && yarn docs:assets && yarn docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/native-element.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "es-dev-server --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"**/*.js\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch",
    "checksize": "rollup -c ; cat native-element.bundled.js | gzip -9 | wc -c ; rm native-element.bundled.js",
    "dev": "rollup -c -w",
    "build": "rollup -c"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript"
  ],
  "author": "mingxianjun",
  "license": "ISC",
  "dependencies": {
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.1.9",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "web-component-analyzer": "^1.0.3"
  },
  "files": [
    "dev",
    "docs-src",
    "elements",
    ".eleventy.cjs",
    ".eslintignore",
    ".eslintrc.json",
    "babel.config.js",
    "custom-elements.json",
    "native-element.bundled.js",
    "native-element.js",
    "rollup.config.js",
    "package.json",
    "README.md"
  ]
}
